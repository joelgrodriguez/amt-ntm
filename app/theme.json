{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "styles": {
    "elements": {
      "link": {
        "typography": {
          "textDecoration": "none"
        },
        ":hover": {
          "typography": {
            "textDecoration": "underline"
          }
        }
      }
    }
  },
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultPalette": false,
      "defaultGradients": false,
      "palette": [
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#0078C2"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#F7951D"
        },
        {
          "name": "Accent",
          "slug": "accent",
          "color": "#B91C1C"
        },
        {
          "name": "Dark",
          "slug": "dark",
          "color": "#18181b"
        },
        {
          "name": "Dark Medium",
          "slug": "dark-medium",
          "color": "#3f3f46"
        },
        {
          "name": "Medium",
          "slug": "medium",
          "color": "#71717a"
        },
        {
          "name": "Light Medium",
          "slug": "light-medium",
          "color": "#d4d4d8"
        },
        {
          "name": "Light",
          "slug": "light",
          "color": "#fafafa"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#ffffff"
        },
        {
          "name": "Border",
          "slug": "border",
          "color": "#e4e4e7"
        }
      ]
    },
    "typography": {
      "fluid": true,
      "fontFamilies": [
        {
          "name": "Sans",
          "slug": "sans",
          "fontFamily": "\"IBM Plex Sans\", ui-sans-serif, system-ui, sans-serif"
        },
        {
          "name": "Serif",
          "slug": "serif",
          "fontFamily": "\"IBM Plex Serif\", ui-serif, Georgia, serif"
        },
        {
          "name": "Mono",
          "slug": "mono",
          "fontFamily": "\"IBM Plex Mono\", ui-monospace, monospace"
        }
      ],
      "fontSizes": [
        {
          "name": "Extra Small",
          "slug": "xs",
          "size": "0.75rem"
        },
        {
          "name": "Small",
          "slug": "sm",
          "size": "0.875rem"
        },
        {
          "name": "Base",
          "slug": "base",
          "size": "1rem"
        },
        {
          "name": "Large",
          "slug": "lg",
          "size": "1.125rem"
        },
        {
          "name": "Extra Large",
          "slug": "xl",
          "size": "1.25rem"
        },
        {
          "name": "2XL",
          "slug": "2xl",
          "size": "1.5rem"
        },
        {
          "name": "3XL",
          "slug": "3xl",
          "size": "1.875rem"
        },
        {
          "name": "4XL",
          "slug": "4xl",
          "size": "2.25rem"
        },
        {
          "name": "5XL",
          "slug": "5xl",
          "size": "3rem"
        },
        {
          "name": "6XL",
          "slug": "6xl",
          "size": "3.75rem"
        },
        {
          "name": "7XL",
          "slug": "7xl",
          "size": "4.5rem"
        },
        {
          "name": "8XL",
          "slug": "8xl",
          "size": "6rem"
        },
        {
          "name": "9XL",
          "slug": "9xl",
          "size": "8rem"
        }
      ]
    },
    "layout": {
      "contentSize": "720px",
      "wideSize": "1440px"
    },
    "custom": {
      "borderRadius": "0"
    }
  }
}
